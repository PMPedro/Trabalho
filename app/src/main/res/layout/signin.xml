<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/background">


    "

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="409dp"
        android:layout_height="33dp"
        android:layout_marginBottom="84dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView59"
        app:layout_constraintVertical_bias="0.797"
        app:srcCompat="@drawable/linha" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="417dp"
        android:layout_height="21dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/linha" />

    <Button
        android:id="@+id/signinbtVoltar"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="644dp"
        android:layout_marginEnd="325dp"
        android:layout_marginBottom="17dp"
        android:background="@android:drawable/ic_dialog_email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/SignInCreateAccount"
        android:layout_width="142dp"
        android:layout_height="53dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/retangulo20"
        android:text="Create Account"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signinBirthdate" />

    <Button
        android:id="@+id/SignInFoto"
        android:layout_width="126dp"
        android:layout_height="126dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/round"
        android:text="Selecionar Foto"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView46" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/selectphoto_imageview"
        android:layout_width="128dp"
        android:layout_height="126dp"
        app:civ_border_width="4dp"
        app:layout_constraintBottom_toTopOf="@+id/SignInEmail"
        app:layout_constraintEnd_toEndOf="@+id/SignInFoto"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/SignInFoto"
        app:layout_constraintTop_toBottomOf="@+id/imageView46"
        app:layout_constraintVertical_bias="0.619" />

    <EditText
        android:id="@+id/SignInPassword"
        android:layout_width="260dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/retangulo21"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        android:paddingLeft="15dp"
        android:textAlignment="viewStart"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SignInName" />

    <EditText
        android:id="@+id/SignInEmail"
        android:layout_width="259dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/retangulo22"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingLeft="15dp"
        android:textAlignment="viewStart"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SignInFoto" />

    <EditText
        android:id="@+id/signinBirthdate"
        android:layout_width="259dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/retangulo23"
        android:ems="10"
        android:hint="Data de Nascimento"
        android:inputType="date"
        android:paddingLeft="15dp"
        android:textAlignment="viewStart"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SignInPassword" />

    <EditText
        android:id="@+id/SignInName"
        android:layout_width="259dp"
        android:layout_height="66dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/retangulo22"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:paddingLeft="15dp"
        android:textAlignment="viewStart"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SignInEmail" />

    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="137dp"
        android:layout_height="84dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/medico" />

</androidx.constraintlayout.widget.ConstraintLayout>
